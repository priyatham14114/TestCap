<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog
        id="myDialog"
        title="Book a Parking Slot"
        class="dialogSlideIn"
        resizable="true"
        draggable="true"
        contentWidth="50%"
        contentHeight="30%"
    >
        <content>
            <form:SimpleForm
            class="popElements"
                id="idBooksDetailsFormToCreate"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <Label
                    id="idAuthorNameLabelCreate2347IUYHAI"
                    text="Driver Name"
                />
                <Input
                    id="idkdjgbrrddrivername"
                    value="{newReserveModel>Drivername}"
                />
                <Label
                    id="idbookNameLabelCreateKHB"
                    text="Driver Mobile"
                />
                <Input
                    id="iddrivrtmobilwInput"
                    value="{newReserveModel>Drivermobile}"

                />
                <Label
                    id="idbookateKHB"
                    text="Vehicle Number"
                />
                <Input
                    id="idameInput"
                    value="{newReserveModel>Vehiclenumber}"

                />
                              
                 <Label
                    id="idboaodkNaasmeLabelCreateKHB"
                    text="Vendor Name"
                />
                <Input
                    id="idvendordadNameInput"
                    value="{newReserveModel>Vendorname}"

                />
            </form:SimpleForm>
        </content>
          <beginButton>
            <Button
                id="idkjwge"
                text="Book"
                press="onBookSlotPress"
            />
        </beginButton>
        <endButton>
            <Button
                id="_IDGenButton1"
                text="Close"
                press="onCloseDialog"
            />
        </endButton>
      
    </Dialog>
</core:FragmentDefinition>